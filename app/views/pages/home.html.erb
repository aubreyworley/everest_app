   <div class="col-md-12 header-title" id="landing-img">
    <div class="container-fluid">
      <h1 class="logo">EVEREST</h1>
      <h2 class="tag-line header-title-hgroup">Elevate your route</h2>
    </div>
  </div>

  <div class="container-fluid">
    <div class="map">
      <div class="col-md-12">
       <div class="nav-header">
        <h1>CREATE YOUR ROUTE:</h1>
      </div>
      <div>
        <form class="form-inline" id="routes">
          <div class="map-nav">
            <div class="form-group">
              <label>
                From: <input id="from" type="text" placeholder="Nob Hill, San Francisco, CA">
              </label>
            </div>
            <div class="form-group" id="from-to-switcher"></div>
            <div class="form-group">
              <label>
                To: <input id="to" type="text" placeholder="Russian Hill, San Francisco, CA">
              </label>
            </div>
            <div class="form-group">
              <label for="travel-mode">Travel mode:</label>
              <select id="travel-mode">
                <option>Bicycling</option>
                <option>Walking</option>
                <option>Driving</option>
              </select>
            </div>
            <div class="form-group">
              <label for="measurement-mode">Units:</label>
              <select id="measurement-mode">
                <option value="miles">Miles</option>
                <option value="km">Kilometres</option>
              </select>
            </div>
            <div class="form-group">
              <button class="btn btn-primary" id="go">Go</button>
            </div>
            <div class="hidden form-group" id="save">
              <label>Save your route?</label>
              <input id="title" type="text" placeholder="My running route">
              <button class="btn btn-primary">Save</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

<div class="col-md-12 charts">
  <div class="col-md-7 map-canvas-container">
    <div>
      <div id="map-canvas"></div>
    </div>
  </div>
  <div class="col-md-2">
    <table class="table">
      <th>
        <td>Difficulty</td>
        <td>% Grade</td>
      </th>
      <tr>
        <td><div class="color-index" id="green"</div></td>
        <td>Easy</td>
        <td>&lt;5%</td>
      </tr>
      <tr>
        <td><div class="color-index" id="yellow"</div></td>
        <td>Moderate</td>
        <td>5-10%</td>
      </tr>
      <tr>
        <td><div class="color-index" id="blue"</div></td>
        <td>Difficult</td>
        <td>10-15%</td>
      </tr>
      <tr>
        <td><div class="color-index" id="red"</div></td>
        <td>Very Difficult</td>
        <td>15-20%</td>
      </tr>
      <tr>
        <td><div class="color-index" id="black"</div></td>
        <td>Too *wheeze* steep</td>
        <td>&gt;20%</td>
      </tr>
    </table>
    <div class="labels">
      Distance:
      <span id="distance"></span>
    </div>
    <div class="labels">
      Travel Time:
      <span id="travel-time"></span>
    </div>
  </div>
  <div class="col-md-3">
    <div class="row">
      <div>
        <div id="elevation_chart"></div>
        <div id="slope_chart"></div>
      </div>
    </div>
  </div>

